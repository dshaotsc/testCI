{
    "description": "Schema for the products which lay in the cart after the first product has been added to the cart",
    "properties": {
      "product-event-type": {
        "description": "Describes the type of product event",
        "type": "string",
        "maxLength": 32,
        "examples": [
          "view", 
          "buy", 
          "c_add", 
          "c_size", 
          "c_whishlist", 
          "crmv", 
          "c_whishlist_rmv", 
          "in_cart"
        ]
      },
      "product-id": {
        "description": "Product id without suffix",
        "type": "string",
        "maxLength": 16,
        "examples": ["123456"]
      },
      "product-master-id": {
        "description": "Master id of product",
        "type": "string",
        "maxLength": 16,
        "examples": ["123456333"]
      },
      "product-suffix": {
        "description": "Suffix of product",
        "type": "string",
        "maxLength": 8,
        "examples": ["22"]
      },
      "product-name": {
        "description": "Name of product in English",
        "type": "string",
        "maxLength": 255,
        "examples": ["dress-with-leopard-skin-print"]
      },
      "product-category": {
        "description": "Associated category of product in English",
        "type": "string",
        "maxLength": 255,
        "examples": ["women/dresses/evening-dresses"]
      },
      "product-source": {
        "description": "Through which element was the product “called/opened” in English",
        "type": "string",
        "maxLength": 32,
        "examples": [
          "cross-sell", 
          "search", 
          "pdp", 
          "plp", 
          "last-viewed item", 
          "wishlist", 
          "cart", 
          "myaccount", 
          "outfit", 
          "direkt-order"
        ]
      },
      "product-rating": {
        "description": "Number of stars displayed in the product rating",
        "type": "integer",
        "maximum": 10,
        "examples": [4, 3]
      },
      "product-color": {
        "description": "Detailled color",
        "type": "string",
        "maxLength": 64,
        "examples": ["mint"]
      },
      "product-color-category": {
        "description": "Color category",
        "type": "string",
        "maxLength": 64,
        "examples": ["green"]
      },
      "product-size-id": {
        "description": "Unified size for all countries",
        "type": "string",
        "maxLength": 8,
        "examples": ["400"]
      },
      "product-size": {
        "description": "Country specific size of the product",
        "type": "string",
        "maxLength": 8,
        "examples": ["400"]
      },
      "product-size-type-select": {
        "description": "Size category",
        "type": "string",
        "maxLength": 32,
        "examples": [
          "size", 
          "short-size", 
          "cup-size"
        ]
      },
      "product-size-id-pim": {
        "description": "Size id of pim",
        "type": "string",
        "maxLength": 8,
        "examples": ["2"]
      },
      "product-status": {
        "description": "Which sales-status does the product have",
        "type": "string",
        "maxLength": 8,
        "examples": [
          "reg", 
          "sale", 
          "new"
        ]
      },
      "product-outfit-id": {
        "description": "Outfit Ids of product",
        "type": "string",
        "maxLength": 16,
        "examples": ["123490"]
      },
      "product-pricing": {
        "description": "Price in EUR",
        "type": "integer",
        "maximum": 2000000,
        "examples": [4995]
      },
      "product-local-pricing": {
        "description": "Price in local curreny (shown to user)",
        "type": "integer",
        "examples": [5995]
      },
      "product-currency": {
        "description": "Name of local currency",
        "type": "string",
        "maxLength": 3,
        "examples": ["CHF"]
      },
      "product-delivery-time": {
        "description": "Delivery time of product in days, sold out or direct delivery",
        "type": "string",
        "maxLength": 16,
        "examples": [
          "sold out", 
          "direct-delivery", 
          "14", 
          "17", 
          "18"
        ]
      },
      "product-availability-status": {
        "description": "Availability status of product if available (Madeleine specific)",
        "type": "string",
        "maxLength": 8,
        "examples": [
          "green", 
          "yellow", 
          "red"
        ]
      },
      "product-amount": {
        "description": "Quantity of product added to cart (for this specific product)",
        "type": "integer",
        "maximum": 50,
        "examples": [1,3,5]
      },
      "product-breadcrumb": {
        "description": "Which breadcrumb did the user see when adding the product",
        "type": "string",
        "maximum": 255,
        "examples": ["neu/mode/blazer"]
      },
      "product-promotion-code": {
        "description": "Is a promotion discount (coupon code) set for this product? If yes which coupon-code does it have",
        "type": "string",
        "maximum": 64,
        "examples": ["123456"]
      }
    },
    "additionalProperties": false,
    "type": "object",
    "required": [
      "product-event-type",
      "product-id",
      "product-master-id",
      "product-suffix",
      "product-name",
      "product-category",
      "product-source",
      "product-color",
      "product-color-category",
      "product-status",
      "product-pricing",
      "product-pricing-currency",
      "product-currency"
    ]
  }
  